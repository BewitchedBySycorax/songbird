{
  "name": "songbird",
  "version": "1.0.0",
  "description": "A small interactive React app",
  "main": "server.js",
  "scripts": {
    "start": "npm run init-db && npx nodemon --exec babel-node server.js",
    "dev": "nodemon npx nodemon --exec babel-node server.js",
    "sequelize-skeleton": "npx sequelize init",
    "init-db": "sequelize db:drop dev_songbird && sequelize db:create dev_songbird && sequelize db:migrate",
    "init-db-sql": "cd /usr/local/var/postgres/base && echo 'DROP DATABASE IF EXISTS dev_songbird; DROP DATABASE IF EXISTS test_songbird; CREATE DATABASE dev_songbird; CREATE DATABASE test_songbird' | psql -d postgres && cd -",
    "lint": "eslint './*.js'",
    "lint:fix": "npm run lint -- --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BewitchedBySycorax/songbird.git"
  },
  "keywords": [],
  "author": "Alexey Voytovsky <al.voytovsky@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/BewitchedBySycorax/songbird/issues"
  },
  "homepage": "https://github.com/BewitchedBySycorax/songbird#readme",
  "dependencies": {
    "body-parser": "^1.19.0",
    "cli-color": "^2.0.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "pg": "^8.3.2",
    "pg-hstore": "^2.3.3",
    "sequelize": "^6.3.4"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-3": "^6.24.1",
    "eslint": "^7.7.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.22.0",
    "nodemon": "^2.0.4",
    "sequelize-cli": "^6.2.0"
  }
}
